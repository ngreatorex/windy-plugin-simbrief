{
    "name": "windy-plugin-simbrief",
    "version": "0.0.1",
    "description": "A Windy plugin to show your SimBrief flighplan on the map",
    "main": "dist/plugin.min.js",
    "type": "module",
    "scripts": {
        "build": "rm -rf dist && mkdir dist && SERVE=false rollup -c && cp package.json dist/",
        "build:win": "rmdir /s /q dist && mkdir dist && set SERVE=false && rollup -c && copy package.json dist\\",
        "start": "cross-env rollup -w -c",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ngreatorex/windy-plugin-simbrief.git"
    },
    "keywords": [
        "windy",
        "windy.com",
        "plugins",
        "simbrief"
    ],
    "author": "Neil Greatorex <neil@greatorex.me.uk>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ngreatorex/windy-plugin-simbrief/issues"
    },
    "homepage": "https://ngreatorex.github.io/windy-plugin-simbrief/",
    "dependencies": {
        "@windycom/plugin-devtools": "^3.0.1",
        "airport-data-js": "^2.0.0",
        "arc": "^0.2.0",
        "leaflet-arc": "^1.0.2",
        "luxon": "^3.7.2"
    },
    "devDependencies": {
        "@types/luxon": "^3.7.1",
        "cross-env": "^10.1.0"
    }
}
